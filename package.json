{
	"name": "webpack-mpa-ts",
	"version": "3.11.0",
	"description": "Multi page app setup with webpack",
	"keywords": [
		"MPA",
		"Multi page",
		"Webpack setup",
		"Webpack config",
		"Webpack boilerplate",
		"Multiple pages application",
		"Multiple pages application setup",
		"Multiple pages application boilerplate"
	],
	"homepage": "https://atanas.info/projects/webpack-mpa-next.html",
	"bugs": {
		"url": "https://github.com/scriptex/webpack-mpa-next/issues",
		"email": "hi@atanas.info"
	},
	"license": "MIT",
	"author": "Atanas Atanasov <hi@atanas.info> (https://atanas.info)",
	"funding": "https://github.com/sponsors/scriptex",
	"main": "assets/scripts/main.js",
	"style": "assets/styles/main.css",
	"repository": {
		"type": "git",
		"url": "github:scriptex/webpack-mpa-next"
	},
	"scripts": {
		"build": "webpack --mode=production",
		"start": "webpack --watch --mode=development",
		"optisize": "optisize --src=\"./assets/images\"",
		"html": "php index.php > index.html",
		"critical": "node critical.mjs",
		"rm-html": "rm index.html",
		"pwa": "create-pwa --icon=\"./assets/images/favicon/icon.png\" --launch=\"./assets/images/favicon/launch.png\"",
		"prod": "yarn build && yarn html && yarn critical && yarn rm-html",
		"lint:ts": "eslint 'assets/scripts/**/*.ts'",
		"lint:scss": "stylelint assets/styles/**/*.scss",
		"lint": "yarn lint:ts && yarn lint:scss"
	},
	"dependencies": {
		"itscss": "3.1.0"
	},
	"devDependencies": {
		"@three11/optisize": "2.0.0",
		"@types/node": "18.11.6",
		"@typescript-eslint/eslint-plugin": "5.41.0",
		"@typescript-eslint/parser": "5.41.0",
		"autoprefixer": "10.4.12",
		"browser-sync": "2.27.10",
		"browser-sync-webpack-plugin": "2.3.0",
		"browserslist": "4.21.4",
		"clean-webpack-plugin": "4.0.0",
		"create-pwa": "2.6.0",
		"critical": "5.0.3",
		"css-loader": "6.7.1",
		"cssnano": "5.1.13",
		"eslint": "8.26.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-recommended": "4.1.0",
		"eslint-plugin-compat": "4.0.2",
		"mini-css-extract-plugin": "2.6.1",
		"node-sass-magic-importer": "5.3.2",
		"postcss": "8.4.18",
		"postcss-cli": "10.0.0",
		"postcss-easy-import": "4.0.0",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-loader": "7.0.1",
		"postcss-merge-rules": "5.1.2",
		"postcss-normalize": "10.0.1",
		"postcss-url": "10.1.3",
		"postcss-utilities": "0.8.4",
		"postcss-watch-folder": "2.0.0",
		"sass": "1.55.0",
		"sass-loader": "13.1.0",
		"source-map-loader": "4.0.1",
		"spritesh": "1.2.1",
		"stylelint": "14.14.0",
		"stylelint-config-recommended": "9.0.0",
		"stylelint-config-standard-scss": "6.0.0",
		"stylelint-no-unsupported-browser-features": "6.0.1",
		"stylelint-scss": "4.3.0",
		"svgo": "3.0.0",
		"ts-loader": "9.4.1",
		"typescript": "4.8.4",
		"webpack": "5.74.0",
		"webpack-cli": "4.10.0",
		"webpack-shell-plugin-next": "2.2.2",
		"webpack-spritesmith": "1.1.0"
	},
	"bin": {
		"wmpats": "./bin/cli.js"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
