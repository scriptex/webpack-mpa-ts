{
	"env": {
		"node": true,
		"browser": true
	},
	"extends": ["prettier", "plugin:@typescript-eslint/recommended", "plugin:compat/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "tsconfig.json",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-explicit-any": "off"
	}
}
